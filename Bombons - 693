#include <stdio.h>

int main() {
	int a, b, c, p, a_q = 0, c_q = 0 , p_q = 0, q, w, e, i;
	float total;
	scanf(" %d", &a);
	scanf(" %d", &c);
	scanf(" %d", &p);
	scanf(" %d", &a_q);
	scanf(" %d", &c_q);
	scanf(" %d", &p_q);
	for(i =0; i < 6; i++){
		q = a_q * a;
		w = c_q * c;
		e = p_q * p;
	}
	total = q + w + e;
	printf("Valor: R$%.2f", total);
	return 0;
}
